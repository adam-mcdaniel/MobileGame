#:kivy 1.0.9

<Sprite>:
    canvas:
        Color:
            rgb: (1, 1, 1)

        Rectangle:
            texture: self.texture
            allow_stretch: True
            pos: self.pos


<ScreenWidget>:
    canvas.before:
        Color:
            rgb: (1,0,0)
        Rectangle:
            size: (self.width,self.height)

